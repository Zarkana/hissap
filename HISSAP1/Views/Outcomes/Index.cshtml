@model IEnumerable<HISSAP1.Models.SiteModels.Outcome>

@{
  ViewBag.Title = "Index";
}

<h2 class="buddy-header">Index</h2>
<div class="buddy-link">
  @Html.ActionLink("Back to List", "Index")
</div>
<p>
  @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
  <tr>
    <th>
      @Html.DisplayNameFor(model => model.DesiredOutcome)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.OutcomeDescription)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.BaselinePercentage)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.DesiredPercentage)
    </th>
    <th></th>
  </tr>

  @foreach (var item in Model)
  {
    <tr>
      <td>
        @Html.DisplayFor(modelItem => item.DesiredOutcome)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.OutcomeDescription)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.BaselinePercentage)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.DesiredPercentage)
      </td>
      <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
      </td>
    </tr>
  }

</table>
